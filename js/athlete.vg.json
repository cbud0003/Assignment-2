{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "height": 300,
    "width": 300,
    "data": {
      "url": "https://raw.githubusercontent.com/cbud0003/FIT3179/main/data/Athlete.csv"
    },
    "mark": {"type": "bar", "cornerRadiusTopLeft": 3, "cornerRadiusTopRight": 3},
    "encoding": {
      "x": {"field": "Athlete", "type": "ordinal"},
      "y": {"field": "Medal_Count", "aggregate": "sum", "title": "Count"},
      "color": {
        "field": "Medal",
        "scale": {"range": ["#BC937A", "#D2BEA6", "#E8E1D1"]}
      },
      "tooltip": [
        {"field": "Medal_Count", "type": "quantitative", "title": "Count"},
        {"field": "Medal", "type": "nominal", "title": "Medal"}
      ]
    },
    "config": {}
  }